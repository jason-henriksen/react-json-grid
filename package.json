{
  "name": "react-es6-npm-component-template",
  "version": "0.0.3",
  "description": "An empty react component for getting started with an NPM published component",
  "main": "dist/index.js",
  "dependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "mobx": "^3.4.1",
    "mobx-react": "^4.3.5",
    "react-autobind": "^1.0.6",
    "react-dimensions": "^1.3.1",
    "react-measure": "^2.0.2",
    "react-scrollbar-size": "^2.0.2",
    "react-tiny-virtual-list": "^2.1.4"
  },
  "peerDependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.3",
    "@storybook/addon-links": "^3.3.3",
    "@storybook/react": "^3.3.3",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.5.1",
    "css-loader": "^0.28.0",
    "custom-react-scripts": "^0.2.1",
    "eslint": "^3.19.0",
    "eslint-config-react-app": "^0.6.2",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "jest": "^19.0.2",
    "path": "^0.12.7",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.4.2",
    "style-loader": "^0.16.1",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  },
  "scripts": {
    "server": "./node_modules/.bin/webpack-dev-server --hot --inline",
    "watch": "webpack --watch ",
    "build": "webpack",
    "start": "webpack --watch",
    "test": "jest",
    "test-coverage": "yarn run jest -- --coverage",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "build-storybook": "build-storybook"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "/node_modules/react",
      "/node_modules/react-dom",
      "/node_modules/react-addons-test-utils"
    ],
    "moduleNameMapper": {
      "^.+\\.(css)$": "<rootDir>/preprocessor.js"
    }
  },
  "author": "Jason Henriksen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jason-henriksen/react-es6-npm-component-template/issues"
  },
  "homepage": "https://github.com/jason-henriksen/react-es6-npm-component-template"
}
